<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="basicApplication.dataClass">

	<class name="CountryHibernate" table="country">
		<id name="countryID" type="integer" column="country_Id">
			<generator class="increment"></generator>
		</id>
		<property name="countryName" type="string" column="country" />
		<property name="update" type="timestamp" column="last_update" />
		
		<!--<set name="cities" table="city_country">
			<key column="cityID" />
			<one-to-many/>
		</set>-->
	</class>

</hibernate-mapping>