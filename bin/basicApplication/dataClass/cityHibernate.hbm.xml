<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="basicApplication.dataClass">

	<class name="CityHibernate" table="city">
		<id name="cityID" type="integer" column="city_Id">
			<generator class="increment"></generator>
		</id>
		<property name="name" type="string" column="city" />
		<property name="countryID" type="integer" column="country_Id" />
		<property name="update" type="timestamp" column="last_update" />
	</class>

</hibernate-mapping>